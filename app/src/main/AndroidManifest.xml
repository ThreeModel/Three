<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asus.threemodel">


    
    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:name=".application.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name="com.example.asus.threemodel.view.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.asus.threemodel.view.activity.BaseActivity" />
        <activity android:name="com.example.asus.threemodel.view.activity.FitActivity" />
        <activity android:name="com.example.asus.threemodel.view.activity.VideoPlayActivity"></activity>
        <activity android:name="com.example.asus.threemodel.view.activity.CollectActivity" />
        <activity android:name="com.example.asus.threemodel.view.activity.BoonActivity" />
        <activity android:name="com.example.asus.threemodel.view.activity.MoreActivity"/>
    </application>

</manifest>